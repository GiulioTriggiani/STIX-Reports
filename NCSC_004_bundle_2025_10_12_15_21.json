        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--52ae644b-31a5-485e-b17b-480bd3a1cb97",
            "created_by_ref": "identity--3cf78822-3626-4eaa-8e85-953c0e88593e",
            "created": "2025-10-12T15:21:41.363962Z",
            "modified": "2025-10-12T15:21:41.363962Z",
            "name": "pygmy goat",
            "description": "Pygmy Goat is a native x86-32 ELF shared object discovered on Sophos XG firewall devices, providing backdoor access by using LD_PRELOAD to inject into /bin/sshd. It hooks the accept function, listens for ICMP packets to trigger C2 connections, and can also use SSH protocol magic bytes for alternative C2. Its capabilities include establishing remote shells, packet capture, cron task creation, and setting up a reverse SOCKS proxy server. Communications are encrypted with TLS using an embedded CA certificate masquerading as Fortinet. The malware is designed for persistence via LD_PRELOAD and can operate on a range of Linux devices.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
        {
