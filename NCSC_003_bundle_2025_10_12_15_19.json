        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--ab7ff0b8-cec0-450b-8686-ea5342178fc5",
            "created_by_ref": "identity--4ffac981-851c-410b-8a7a-d22e09c08509",
            "created": "2025-10-12T15:19:51.769073Z",
            "modified": "2025-10-12T15:19:51.769073Z",
            "name": "damascened peacock",
            "description": "DAMASCENED PEACOCK is a lightweight, staged downloader malware targeting Windows systems, delivered via spear-phishing. It acts as the second stage in a three-stage infection chain, responsible for downloading and executing the final payload via a COM Hijack. It implements defense evasion techniques such as XOR-based string obfuscation and dynamic Win32 API resolution, and is proxy-aware, communicating with its C2 server over HTTP.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--2b4eec5e-4526-4b85-96d1-9938b4839e87",
            "created": "2025-10-12T15:19:51.769612Z",
            "modified": "2025-10-12T15:19:51.769612Z",
            "name": "UK Ministry of Defence",
            "description": "The UK Ministry of Defence (MOD) was the explicit victim of a spear-phishing campaign delivering DAMASCENED PEACOCK malware.",
            "identity_class": "organization"
        },
        {
