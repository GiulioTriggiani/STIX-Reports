        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--f04a73bb-8b23-4102-ac91-db24107b3a44",
            "created_by_ref": "identity--48d7d991-a25b-4747-8f21-c30a6b1f6134",
            "created": "2025-09-18T09:45:21.072426Z",
            "modified": "2025-09-18T09:45:21.072426Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware targeting Outlook on Windows. It runs within the Outlook process, displaying malicious login prompts to steal credentials and OAuth 2.0 tokens, which are then used to access victim email accounts. The malware employs extensive defense evasion techniques such as environmental keying, string obfuscation, and unhooking of ntdll.dll registry APIs. It masquerades as the Microsoft Authentication Library (MSAL) for .NET, embedding the codebase with added malicious classes. Exfiltration is performed by sending emails from the victim\u2019s account to an actor-controlled address, with communications exclusively to legitimate services. Persistence is achieved via COM hijacking, and the malware includes both a dropper and a stealer payload, as well as associated PowerShell scripts.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--1f7015f0-9c18-495e-b528-23967b00974f",
