        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--2f686ff4-bb8f-43b9-93d2-960ddd8071dc",
            "created_by_ref": "identity--d9a8dc5c-0c51-40e6-bc24-02a19231dae8",
            "created": "2025-09-02T15:37:46.812245Z",
            "modified": "2025-09-02T15:37:46.812245Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware targeting Outlook on Windows. It runs within the Outlook process, displaying malicious login prompts to steal credentials and OAuth 2.0 tokens, which can be used to access victim email accounts. The malware employs extensive defense evasion techniques such as environmental keying, string obfuscation, and unhooking registry APIs in ntdll.dll. It consists of a dropper (Microsoft.Identity64.dll), a stealer payload (Microsoft.Identity.Client.dll), and associated PowerShell scripts. The dropper launches the stealer payload, which generates login prompts and intercepts authentication requests to steal credentials and tokens. Exfiltration is performed by sending emails from the victim\u2019s account to an actor-controlled email address, with data obfuscated using gzip compression and RSA encryption. Persistence is achieved via COM hijacking, and the malware communicates exclusively with legitimate services, without traditional command and control.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--65f83757-a3fb-4c43-8cbe-ee572c3db74b",
            "created_by_ref": "identity--d9a8dc5c-0c51-40e6-bc24-02a19231dae8",
            "created": "2025-09-02T15:37:46.812677Z",
            "modified": "2025-09-02T15:37:46.812677Z",
            "name": "microsoft.identity64.dll",
            "description": "Microsoft.Identity64.dll is the dropper component of AUTHENTIC ANTICS, responsible for implementing defense evasion techniques, decrypting, and loading the credential stealing payload (Microsoft.Identity.Client.dll). It is a 64-bit DLL built as a mixed-mode assembly containing both C++ and .NET code. The dropper executes and persists via COM hijacking and carries out significant defense evasion measures before decrypting and running the stealer.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--8c3cab7e-af62-4b8e-a790-7430d44679fa",
            "created_by_ref": "identity--d9a8dc5c-0c51-40e6-bc24-02a19231dae8",
            "created": "2025-09-02T15:37:46.81299Z",
            "modified": "2025-09-02T15:37:46.81299Z",
            "name": "microsoft.identity.client.dll",
            "description": "Microsoft.Identity.Client.dll is the credential and OAuth 2.0 token stealer payload loaded by Microsoft.Identity64.dll. It is a 64-bit .NET DLL that generates login prompts to authenticate to Microsoft\u2019s OAuth 2.0 Authorization server and intercepts login requests to steal credentials and tokens. The stolen data is exfiltrated by authenticating to the victim\u2019s Outlook account and sending emails to an actor-controlled address.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
