        {
            "type": "malware",
            "id": "malware--3bff34a3-c501-4b96-ab20-29f38bb918ed",
            "name": "COLDSTEEL",
            "description": "COLDSTEEL is a fully featured Windows remote access tool (RAT) providing interactive desktop and command-line invocation, file copy, screenshot capture, and user input simulation. It persists as a Windows service and communicates with its C2 server using a raw TCP connection, often using obfuscation. Multiple variants exist, including MileStone2016, MileStone2017, and FBI20111024.",
            "malware_types": [
                "remote-access-trojan"
            ],
            "is_family": "true",
            "labels": [
                "rat",
                "windows",
                "remote-access-trojan",
                "backdoor",
                "obfuscation",
                "persistence",
                "c2"
            ],
            "created": "2023-01-31T00:00:00Z",
            "modified": "2023-01-31T00:00:00Z",
            "external_references": [
                {
                    "source_name": "Ahnlab",
