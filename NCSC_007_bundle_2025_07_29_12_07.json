                }
            ]
        },
        {
            "type": "malware",
            "id": "malware--c5ba2e87-7107-480b-b267-3140201392f2",
            "name": "COLDSTEEL MileStone2017",
            "description": "MileStone2017 is a variant of COLDSTEEL that supports Windows 10, typically protected with Themida, and uses unique IDs in network communication. It communicates over raw TCP, supports interactive desktop and command-line access, and persists as a Windows service named msupdate or msupdate2.",
            "malware_types": [
                "remote-access-trojan"
            ],
            "is_family": "false",
