        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--0a716c3d-632e-4b32-ade7-d95428b15a78",
            "created_by_ref": "identity--12210fce-ad41-4854-a6f1-517ed48d8667",
            "created": "2025-09-09T09:37:50.660217Z",
            "modified": "2025-09-09T09:37:50.660217Z",
            "name": "damascened peacock",
            "description": "DAMASCENED PEACOCK is a lightweight, staged downloader targeting Windows systems, delivered via spear-phishing campaigns. It is implemented as a 32-bit DLL with a single export and is responsible for downloading and executing a final stage payload via a COM Hijack. The malware employs defense evasion techniques such as XOR-based string obfuscation and dynamic API resolution, and communicates with its C2 server over HTTP. It is a variant of RustyClaw and SnipBot, and part of the wider RomCom malware family.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
