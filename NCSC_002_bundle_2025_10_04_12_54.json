        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--f88fbde6-292e-493f-9752-818c3762fbf4",
            "created_by_ref": "identity--e655df58-c495-4daa-94b7-c4826e8b3cf9",
            "created": "2025-10-04T12:54:49.597438Z",
            "modified": "2025-10-04T12:54:49.597438Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware targeting Outlook on Windows. It runs within the Outlook process, displaying malicious login prompts to steal credentials and OAuth 2.0 tokens, which are then used to access victim email accounts. The malware employs extensive defense evasion techniques, including environmental keying, string obfuscation, and unhooking registry APIs in ntdll.dll. It masquerades as the Microsoft Authentication Library (MSAL) for .NET, includes legitimate code for appearance, and exfiltrates stolen data by sending emails from the victim\u2019s account to an actor-controlled address, avoiding detection by not saving sent items. Persistence is achieved via COM hijacking, and communications are exclusively with legitimate services.",
            "malware_types": [
                "spyware",
                "remote-access-trojan"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
