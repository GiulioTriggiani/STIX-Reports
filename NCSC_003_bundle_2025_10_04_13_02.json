        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--b61428f3-5fec-4c1c-9881-35cc9669a89a",
            "created_by_ref": "identity--50b9e9a5-ec8d-4439-85b9-5c2c8f8b2735",
            "created": "2025-10-04T13:02:19.190534Z",
            "modified": "2025-10-04T13:02:19.190534Z",
            "name": "damascened peacock",
            "description": "DAMASCENED PEACOCK is a lightweight, staged downloader targeting Windows, delivered via spear-phishing. It is implemented as a 32-bit DLL with a single export named DllExport containing the malicious functionality. The malware is responsible for downloading the final stage and executing it via a COM Hijack. It uses defence evasion techniques such as XOR based string obfuscation and dynamic resolution of Win32 APIs. DAMASCENED PEACOCK is a variant of RustyClaw and SnipBot, and part of the wider RomCom malware family. It is proxy aware, uses WinHTTP APIs to communicate with its C2 server over HTTP, and persists by hijacking the explorer.exe process.",
            "malware_types": [
                "downloader",
                "backdoor"
            ],
            "is_family": true
        },
