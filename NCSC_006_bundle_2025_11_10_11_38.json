        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--695c4cb8-538f-4c06-8871-278226c5b128",
            "created_by_ref": "identity--2bbea7e5-df6f-4db5-b7cd-79463c43aaaf",
            "created": "2025-11-10T11:38:51.248875Z",
            "modified": "2025-11-10T11:38:51.248875Z",
            "name": "jaguar tooth",
            "description": "Jaguar Tooth is non-persistent malware that targets Cisco IOS routers. It collects device information via hard-coded Cisco IOS CLI and Tcl commands and exfiltrates this data over TFTP. Additionally, it enables unauthenticated backdoor access by patching authentication routines, allowing access to local accounts without password verification via Telnet and physical sessions. It is deployed through exploitation of the SNMP vulnerability CVE-2017-6742, using custom shellcode written to memory.",
            "malware_types": [
                "backdoor",
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "vulnerability",
            "spec_version": "2.1",
            "id": "vulnerability--ecc7b544-8d35-46a2-8781-0de13da87b4e",
            "created_by_ref": "identity--2bbea7e5-df6f-4db5-b7cd-79463c43aaaf",
            "created": "2025-11-10T11:38:51.249328Z",
            "modified": "2025-11-10T11:38:51.249328Z",
            "name": "CVE-2017-6742",
            "description": "A patched Simple Network Management Protocol (SNMP) vulnerability in Cisco IOS routers which allows a stack-based buffer to be overflowed via a specially crafted OID, enabling remote code execution and deployment of the Jaguar Tooth malware."
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--7c0b2cc7-6f92-4cfb-963a-f7f85384d833",
            "created": "2025-11-10T11:38:51.249623Z",
            "modified": "2025-11-10T11:38:51.249623Z",
            "name": "Cisco",
            "description": "Vendor whose IOS routers are targeted by Jaguar Tooth and the publisher of the advisory for CVE-2017-6742.",
            "identity_class": "organization"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--83cfd487-77b2-41f9-8945-2683f32a5654",
            "created_by_ref": "identity--2bbea7e5-df6f-4db5-b7cd-79463c43aaaf",
            "created": "2025-11-10T11:38:51.249903Z",
            "modified": "2025-11-10T11:38:51.249903Z",
            "name": "JaguarTooth_Cisco_IOS_payload",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule JaguarTooth_Cisco_IOS_payload {\n    meta:\n        author = \"NCSC\"\n        description = \"This signature detects the Jaguar Tooth Cisco IOS\nmalware. It looks for the process name, two of the hard-coded commands,\nand code calling two Cisco IOS functions.\"\n    strings:\n        $ = \"Service Policy Lock\"\n        $ = \"sleep 5000\"\n        $ = \"tclquit\"\n        $ = {0C ?? ?? ?? 00 00 30 25 0C ?? ?? ?? 24 04 FF FF 8F BF 00 34}\n    condition:\n        3 of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-10T11:38:51.24989Z"
        },
