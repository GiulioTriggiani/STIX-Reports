        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--bbdbb68d-ff1b-4365-94aa-ca040253d525",
            "created_by_ref": "identity--2efc5d71-6942-4f91-b70c-cca84c72937c",
            "created": "2025-09-18T09:58:17.138251Z",
            "modified": "2025-09-18T09:58:17.138251Z",
            "name": "pygmy goat",
            "description": "Pygmy Goat is a native x86-32 ELF shared object discovered on Sophos XG firewall devices, providing backdoor access. It uses LD_PRELOAD to inject into /bin/sshd, hooks the accept function, and listens for ICMP packets or SSH connections with magic bytes to trigger C2 callbacks. Functionality includes remote shell access, packet capture, cron task creation, and establishing a reverse SOCKS proxy server. It uses AES-encrypted ICMP packets for C2, verifies C2 TLS certificates with an embedded CA masquerading as Fortinet, and employs Unix sockets for inter-process communication. The malware is sophisticated, cleanly coded, and designed for extensibility, with evidence suggesting it could target a range of Linux devices.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--5ab184e7-ae01-4deb-8cd9-bfe25d35a8ce",
            "created_by_ref": "identity--2efc5d71-6942-4f91-b70c-cca84c72937c",
            "created": "2025-09-18T09:58:17.138645Z",
            "modified": "2025-09-18T09:58:17.138645Z",
            "name": "libsophos.so",
            "description": "libsophos.so is the malicious shared object loaded into /bin/sshd by Pygmy Goat via LD_PRELOAD. It contains the main backdoor functionality, including hooking the accept function, establishing C2 via ICMP or SSH, and supporting remote shell, packet capture, cron tasks, and reverse SOCKS proxy. Earlier variants lacked the reverse proxy feature and used VMProtect for obfuscation.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
