        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--739e0c8f-b8b1-47e2-90b8-244c5cb9da42",
            "created_by_ref": "identity--f01516f8-d2e3-4ea0-afd5-dfb79d492807",
            "created": "2025-09-09T09:27:59.039427Z",
            "modified": "2025-09-09T09:27:59.039427Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware that targets the Windows Operating System, specifically running within the Outlook process. It displays malicious login prompts to steal credentials and OAuth 2.0 tokens, which are then used to access victim email accounts. The malware employs extensive defense evasion techniques, including environmental keying, string obfuscation, and unhooking of registry APIs in ntdll.dll. It masquerades as the Microsoft Authentication Library (MSAL) for .NET, includes a dropper and stealer payload, and uses COM hijacking for persistence. Exfiltration is performed by sending emails from the victim\u2019s account to an actor-controlled email address, with data obfuscated using gzip and RSA encryption. Network communications are exclusively with legitimate services, and the malware does not implement traditional command and control.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--f29b3d05-f493-4ba2-968c-17f576956dc3",
