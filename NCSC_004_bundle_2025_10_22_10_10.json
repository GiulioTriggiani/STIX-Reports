        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--af8c6a22-e33a-4222-a2bb-582a16147986",
            "created_by_ref": "identity--c52fa6bd-8a31-4b34-8a4f-51aed811503b",
            "created": "2025-10-22T10:10:42.253699Z",
            "modified": "2025-10-22T10:10:42.253699Z",
            "name": "pygmy goat",
            "description": "Pygmy Goat is a native x86-32 ELF shared object backdoor discovered on Sophos XG firewall devices that uses LD_PRELOAD to inject into /bin/sshd and hook the accept function. It listens for AES-encrypted ICMP packets to obtain a C2 IP/port for TLS connect-back or inspects incoming SSH connections for specific magic bytes to reuse the session. Capabilities include remote /bin/sh and /bin/csh shells, packet capture via libpcap, creating cron tasks through an embedded BusyBox crontab, and establishing a reverse SOCKS5 proxy using embedded EarthWorm code. It verifies C2 via a hardcoded Root CA certificate claiming FortiGate/Fortinet, and compresses command traffic with LZO1X. Older variants used VMProtect and lacked reverse proxy functionality.",
            "malware_types": [
                "backdoor",
                "remote-access-trojan"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
