            "name": "DevilBait_Maldoc",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule DevilBait_Maldoc { meta: author = \"NCSC\" description = \"These strings appear in the Devil Bait malicious document.\" strings: $word = \"MSWordDoc\" $ms_xml = \"MSXML2.ServerXMLHTTP.6.0\" $ = {53 65 6E 64 3A 45 78 65 63 75 74 65 28 [1-6] 2E 72 65 73 70 6F 6E 73 65 54 65 78 74 29} $ = \"wscript.exe //e:vbscript\" condition: all of them }",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T16:14:56.347321Z"
        },
