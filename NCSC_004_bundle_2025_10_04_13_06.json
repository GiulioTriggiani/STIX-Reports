        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--17e8d554-79f6-4353-8c15-4aa6ac3e9515",
            "created_by_ref": "identity--444aaad5-abb8-434c-b8e0-6afd00929dc5",
            "created": "2025-10-04T13:06:43.906851Z",
            "modified": "2025-10-04T13:06:43.906851Z",
            "name": "pygmy goat",
            "description": "Pygmy Goat is a native x86-32 ELF shared object discovered on Sophos XG firewall devices, providing backdoor access. It uses LD_PRELOAD to inject into /bin/sshd, hooks the accept function, and listens for ICMP packets to trigger C2 connections. Its capabilities include remote shell access, packet capture, cron task creation, and establishing a reverse SOCKS proxy server. It uses AES-encrypted ICMP packets for C2, masquerades its TLS certificate as Fortinet, and is designed for extensibility and stealth, blending with normal network traffic.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--023a24fa-57dd-4085-bb58-205aa0b23a5a",
            "created_by_ref": "identity--444aaad5-abb8-434c-b8e0-6afd00929dc5",
            "created": "2025-10-04T13:06:43.907468Z",
            "modified": "2025-10-04T13:06:43.907468Z",
            "name": "libsophos.so",
            "description": "Malicious shared object loaded into /bin/sshd by Pygmy Goat. It replaces functions in the sshd binary via LD_PRELOAD, hooks the accept function, and facilitates backdoor access. Earlier variants lacked reverse proxy functionality and used VMProtect for obfuscation.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
