        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--3ee114d3-ce0b-44f2-a5e4-e4609bdfbddc",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.447719Z",
            "modified": "2025-11-11T22:19:57.447719Z",
            "name": "authentic antics",
            "description": "A highly targeted credential and OAuth 2.0 token-stealing malware designed to target the Windows Operating System and specifically the Outlook process. It displays periodic malicious login prompts to intercept and exfiltrate Microsoft Office account credentials and tokens, which can be used for later access to services like Exchange Online, SharePoint, and OneDrive. The malware employs extensive defense evasion techniques, such as environmental keying and unhooking ntdll.dll, and exfiltrates data by sending emails from the victim's account to an actor-controlled address.",
            "malware_types": [
                "spyware",
                "remote-access-trojan"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--163c9492-ee60-4f79-928f-c1b7b4471d33",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.44809Z",
            "modified": "2025-11-11T22:19:57.44809Z",
            "name": "microsoft.identity64.dll",
            "description": "The dropper component of AUTHENTIC ANTICS. It is a 64-bit mixed-mode assembly (C++ and .NET) DLL responsible for implementing defence evasion techniques, decrypting, and loading the credential stealing payload, Microsoft.Identity.Client.dll.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--4c800270-2442-453a-a5dd-50a08f71b088",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.448402Z",
            "modified": "2025-11-11T22:19:57.448402Z",
            "name": "microsoft.identity.client.dll",
            "description": "The stealer payload of AUTHENTIC ANTICS, which is a 64-bit .NET DLL. It is loaded directly into memory by Microsoft.Identity64.dll and is not present on disk. This component is responsible for generating login prompts to steal credentials and OAuth 2.0 tokens.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--25150fe6-c72f-46f0-ad2e-3766f6ccf771",
            "created": "2025-11-11T22:19:57.448711Z",
            "modified": "2025-11-11T22:19:57.448711Z",
            "name": "Microsoft",
            "description": "Provider of the software and services targeted by AUTHENTIC ANTICS, including Outlook, Office, Azure Active Directory Authentication Library (ADAL), and OAuth 2.0 Authorization services. The malware's stealer payload masquerades as Microsoft's Authentication Library (MSAL).",
            "identity_class": "organization"
        },
        {
            "type": "identity",
