        {
            "type": "threat-actor",
            "spec_version": "2.1",
            "id": "threat-actor--7005ff3f-391d-40c9-af77-b944ff644207",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.44918Z",
            "modified": "2025-11-11T22:19:57.44918Z",
            "name": "AUTHENTIC ANTICS actor",
            "description": "The actor responsible for creating and deploying the AUTHENTIC ANTICS malware. Their primary goal is to gain long-term persistent access to victim email accounts. They employ extensive defense evasion and stealth techniques, including COM hijacking for persistence, unhooking ntdll.dll functions, environmental keying, and exfiltrating data exclusively through legitimate services like the Outlook Web API to avoid detection."
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--e474142d-0bdb-43f2-84a1-8b6af5b12e73",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.449496Z",
            "modified": "2025-11-11T22:19:57.449496Z",
            "name": "AUTHENTIC_ANTICS_obfuscated_stack_strings",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_obfuscated_stack_strings {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects patterns in how the AUTHENTIC ANTICS dropper\nbuilds strings on the stack.\"\n\n    strings:\n        $stack_string_1 = {8D 50 ?? 8D 48 ?? 8D 78 ?? 8D 42 ?? 44 8D 42 ?? 44 8D 62 ??\n44 8D 52 ?? 44 8D 6A } // build obfuscated stack string\n        $stack_string_2 = {8D 73 ?? 44 8D 4B ?? 44 8D 53 ?? 44 8D 5B ?? 44 8D 73 ?? 44\n8D 7B ??}\n        $stack_string_3 = {41 8D 49 ?? 8D 51 ?? 45 8D 41 ?? 45 8D 51 ?? 45 8D 59}\n\n    condition:\n\n    uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them\n\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T22:19:57.449484Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--ed2ebaf4-0de2-4172-9ab3-014f44d566e1",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.44981Z",
            "modified": "2025-11-11T22:19:57.44981Z",
            "name": "AUTHENTIC_ANTICS_ntdll_unhooking",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_ntdll_unhooking {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects code used in the unhooking of ntdll functions\nin the AUTHENTIC ANTICS dropper.\"\n\n    strings:\n        $addr_calc = {48 8B D0 49 2B D4 8B D2 49 03 D6 48 8B 00 48 3B 02} // ntdll\nfunction unhooking calculation logic\n\n    condition:\n        uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them\n\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T22:19:57.449802Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--47571d27-53c7-4737-8306-2792d36fc885",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.450075Z",
            "modified": "2025-11-11T22:19:57.450075Z",
            "name": "AUTHENTIC_ANTICS_pdb_and_imports",
            "description": "YARA rule to detect related activity.",
            "pattern": "import \"pe\"\n\nrule AUTHENTIC_ANTICS_pdb_and_imports {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects the pdb path and imports associated with the\nAUTHENTIC ANTICS dropper.\"\n\n    strings:\n        $pdb = \"C:\\\\Users\\\\user\\\\Desktop\\\\outlook4\\\\bin\\\\\"\n\n    condition:\n        uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them and\npe.imports(\"user32.dll\",\"GetClassNameW\") and pe.imports(\"user32.dll\",\"GetParent\") and\npe.imports(\"user32.dll\",\"EnumChildWindows\") and\npe.imports(\"user32.dll\",\"GetWindowTextW\") and\npe.imports(\"user32.dll\",\"GetWindowThreadProcessId\")\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T22:19:57.450066Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--6df44118-06d3-4bf3-b5d9-0f2fc502d4c7",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.450364Z",
            "modified": "2025-11-11T22:19:57.450364Z",
            "name": "AUTHENTIC_ANTICS_iertutil_hook",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_iertutil_hook{\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects code which is used to implement a hook of\niertutil.dll by the AUTHENTIC ANTICS dropper.\"\n\n    strings:\n        $stub_func = {81 F9 04 00 00 20 ?? ?? B8 01 00 00 00 C3 81 F9 01 00 00 20 ?? ??\n33 C0 C3} // used as hook\n        $import_check = {49 63 44 24 3C 42 83 BC 20 94 00 00 00 00 0F}\n        $calc_address = {41 8B 5E 10 49 03 DC 41 8B 3E 49 03 FC}\n\n    condition:\n        uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T22:19:57.450355Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--db80d63b-75ad-4288-b611-aaae0163bb02",
            "created_by_ref": "identity--2863dcf8-6f27-42a5-9647-8090fce3f663",
            "created": "2025-11-11T22:19:57.450646Z",
            "modified": "2025-11-11T22:19:57.450646Z",
            "name": "AUTHENTIC_ANTICS_Stealer",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_Stealer {\n    meta:\n        author = \"NCSC\"\n\n description = \"This rule detects strings present in the AUTHENTIC ANTICS .NET\n\nstealer payload.\"\n\n    strings:\n        $ = \"http://www.gstatic.com/generate_204\" wide\n        $ = \"d3590ed6-52b3-4102-aeff-aad2292ab01c\" wide\n        $ = \"https://login.microsoftonline.com/common/oauth2/token\" wide\n        $ = \"https://outlook.office365.com/api/v2.0/me/sendMail\" wide\n        $ =\n\"https://login.microsoftonline.com/common/oauth2/authorize?response_type=code+id_token&c\nlient_id=d3590ed6-52b3-4102-aeff-aad2292ab01c&redirect_uri=ms-appx-\nweb%3a%2f%2fMicrosoft.AAD.BrokerPlugin%2fd3590ed6-52b3-4102-aeff-\naad2292ab01c&instance_aware=true&nonce=\" wide\n\nweb://Microsoft.AAD.BrokerPlugin/d3590ed6-52b3-4102-aeff-aad2292ab01c\" wide\n\n$ = \"<form method=\\\"POST\\\" name=\\\"hiddenform\\\" action=\\\"ms-appx-\n\n    condition:\n        (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550 and all of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T22:19:57.450636Z"
        },
