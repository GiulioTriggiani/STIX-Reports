        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--6b88b43a-883e-4dd3-a1f3-76788251ada0",
            "created_by_ref": "identity--f87ef615-5e0a-4b66-8e50-2b49c506e305",
            "created": "2025-10-22T13:08:34.096745Z",
            "modified": "2025-10-22T13:08:34.096745Z",
            "name": "pygmy goat",
            "description": "Pygmy Goat is a native x86-32 ELF shared object backdoor discovered on Sophos XG firewall devices. It leverages the LD_PRELOAD environment variable to inject into /bin/sshd and hook the accept() function, giving the actor on-demand C2 access. It listens on a raw ICMP socket for AES-256-CBC encrypted port-knock packets containing magic bytes and a callback IPv4 address/port, and alternatively triggers via a hooked SSH accept path that recognizes specific version bytes and performs a fake SSH handshake before upgrading to TLS. The sample verifies peer certificates against an embedded Root CA claiming FortiGate, Fortinet Ltd., then processes LZO1X-compressed tasking to provide remote shells (/bin/sh, /bin/csh), libpcap-based packet capture, interactive BusyBox crontab for scheduled tasks, and creation of a reverse SOCKS5 proxy using embedded EarthWorm code. It maintains single-instance locking, uses Unix IPC sockets (/tmp/.sshd.ipc and .fgmon_cli.ipc), and indicates persistence by setting LD_PRELOAD on boot.",
            "malware_types": [
                "backdoor",
                "remote-access-trojan"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--a29395a4-6319-43a9-883e-4abb4edc1385",
            "created_by_ref": "identity--f87ef615-5e0a-4b66-8e50-2b49c506e305",
            "created": "2025-10-22T13:08:34.097113Z",
            "modified": "2025-10-22T13:08:34.097113Z",
            "name": "castletap",
            "description": "Referenced in Mandiant reporting on FortiGate device attacks. CASTLETAP exhibits TTPs similar to Pygmy Goat, including an encrypted ICMP packet carrying C2 information used to establish a reverse SSL connection.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
