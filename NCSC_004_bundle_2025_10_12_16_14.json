        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--6232b9ef-cb9c-4e9f-bbbc-1a5a0167600a",
            "created_by_ref": "identity--54db854a-2479-4f11-a63b-d7675dcbecb0",
            "created": "2025-10-12T16:14:05.349286Z",
            "modified": "2025-10-12T16:14:05.349286Z",
            "name": "pygmy goat",
            "description": "Pygmy Goat is a native x86-32 ELF shared object discovered on Sophos XG firewall devices, providing backdoor access by using LD_PRELOAD to inject into /bin/sshd. It listens for ICMP packets or specific SSH connections to trigger C2 callbacks, establishes TLS connections using a forged Fortinet CA certificate, and supports remote shell, packet capture, cron task creation, and reverse SOCKS proxying via EarthWorm. It is capable of persistence via LD_PRELOAD, hooks the accept function in sshd, and is extensible for use on a range of Linux devices.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--53ff750d-2271-4c7e-8ae0-668b6317f619",
            "created": "2025-10-12T16:14:05.349645Z",
            "modified": "2025-10-12T16:14:05.349645Z",
            "name": "Sophos XG Firewall",
            "description": "Sophos XG Firewall devices are the primary victims targeted by the Pygmy Goat malware, which provides backdoor access to these network devices.",
            "identity_class": "organization"
        },
        {
