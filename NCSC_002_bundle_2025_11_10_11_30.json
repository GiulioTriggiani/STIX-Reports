        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--5d17e8a8-800a-4ca6-828b-9042cdacbcd5",
            "created_by_ref": "identity--ee743296-07e6-4d7b-8fcd-2efb4fbd013a",
            "created": "2025-11-10T11:30:37.290793Z",
            "modified": "2025-11-10T11:30:37.290793Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware focused on Outlook. Running as a 64-bit mixed-mode DLL within the Outlook process, it displays malicious login prompts, steals credentials and tokens, and uses defense evasion techniques such as environmental keying and removing suspected API hooks from ntdll.dll. Network communications are exclusively with legitimate services, with exfiltration via emails sent from the victim's Outlook account. The malware also persists via COM hijacking and employs both a .NET stealer payload and support PowerShell scripts to accomplish its objectives.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--765041fb-2f5d-419f-a6aa-bf548acbcdbb",
            "created": "2025-11-10T11:30:37.291179Z",
            "modified": "2025-11-10T11:30:37.291179Z",
            "name": "NCC Group",
            "description": "Provided AUTHENTIC ANTICS malware samples to NCSC for analysis.",
            "identity_class": "organization"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--deee9f6c-cd8b-4fa3-85ed-6cff3e8aa342",
            "created_by_ref": "identity--ee743296-07e6-4d7b-8fcd-2efb4fbd013a",
            "created": "2025-11-10T11:30:37.291449Z",
            "modified": "2025-11-10T11:30:37.291449Z",
            "name": "AUTHENTIC_ANTICS_obfuscated_stack_strings",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_obfuscated_stack_strings {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects patterns in how the AUTHENTIC ANTICS dropper\nbuilds strings on the stack.\"\n\n    strings:\n        $stack_string_1 = {8D 50 ?? 8D 48 ?? 8D 78 ?? 8D 42 ?? 44 8D 42 ?? 44 8D 62 ??\n44 8D 52 ?? 44 8D 6A } // build obfuscated stack string\n        $stack_string_2 = {8D 73 ?? 44 8D 4B ?? 44 8D 53 ?? 44 8D 5B ?? 44 8D 73 ?? 44\n8D 7B ??}\n        $stack_string_3 = {41 8D 49 ?? 8D 51 ?? 45 8D 41 ?? 45 8D 51 ?? 45 8D 59}\n\n    condition:\n\n    uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them\n\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-10T11:30:37.291434Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--6b0b3458-7062-4c4d-a491-ae54d744dd38",
            "created_by_ref": "identity--ee743296-07e6-4d7b-8fcd-2efb4fbd013a",
            "created": "2025-11-10T11:30:37.291747Z",
            "modified": "2025-11-10T11:30:37.291747Z",
            "name": "AUTHENTIC_ANTICS_ntdll_unhooking",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_ntdll_unhooking {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects code used in the unhooking of ntdll functions\nin the AUTHENTIC ANTICS dropper.\"\n\n    strings:\n        $addr_calc = {48 8B D0 49 2B D4 8B D2 49 03 D6 48 8B 00 48 3B 02} // ntdll\nfunction unhooking calculation logic\n\n    condition:\n        uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them\n\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-10T11:30:37.291729Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--dc358dc9-6496-4a0d-99c6-0f504a270144",
            "created_by_ref": "identity--ee743296-07e6-4d7b-8fcd-2efb4fbd013a",
            "created": "2025-11-10T11:30:37.292033Z",
            "modified": "2025-11-10T11:30:37.292033Z",
            "name": "AUTHENTIC_ANTICS_pdb_and_imports",
            "description": "YARA rule to detect related activity.",
            "pattern": "import \"pe\"\n\nrule AUTHENTIC_ANTICS_pdb_and_imports {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects the pdb path and imports associated with the\nAUTHENTIC ANTICS dropper.\"\n\n    strings:\n        $pdb = \"C:\\\\Users\\\\user\\\\Desktop\\\\outlook4\\\\bin\\\\\"\n\n    condition:\n        uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them and\npe.imports(\"user32.dll\",\"GetClassNameW\") and pe.imports(\"user32.dll\",\"GetParent\") and\npe.imports(\"user32.dll\",\"EnumChildWindows\") and\npe.imports(\"user32.dll\",\"GetWindowTextW\") and\npe.imports(\"user32.dll\",\"GetWindowThreadProcessId\")\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-10T11:30:37.292025Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--211358b3-ae57-4cc3-ad14-9b1197fa6146",
            "created_by_ref": "identity--ee743296-07e6-4d7b-8fcd-2efb4fbd013a",
            "created": "2025-11-10T11:30:37.292316Z",
            "modified": "2025-11-10T11:30:37.292316Z",
            "name": "AUTHENTIC_ANTICS_iertutil_hook",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_iertutil_hook{\n    meta:\n        author = \"NCSC\"\n        description = \"This rule detects code which is used to implement a hook of\niertutil.dll by the AUTHENTIC ANTICS dropper.\"\n\n    strings:\n        $stub_func = {81 F9 04 00 00 20 ?? ?? B8 01 00 00 00 C3 81 F9 01 00 00 20 ?? ??\n33 C0 C3} // used as hook\n        $import_check = {49 63 44 24 3C 42 83 BC 20 94 00 00 00 00 0F}\n        $calc_address = {41 8B 5E 10 49 03 DC 41 8B 3E 49 03 FC}\n\n    condition:\n        uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and all of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-10T11:30:37.292309Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--91f46022-59cf-4db2-953c-e99fb74c4f66",
            "created_by_ref": "identity--ee743296-07e6-4d7b-8fcd-2efb4fbd013a",
            "created": "2025-11-10T11:30:37.292588Z",
            "modified": "2025-11-10T11:30:37.292588Z",
            "name": "AUTHENTIC_ANTICS_Stealer",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule AUTHENTIC_ANTICS_Stealer {\n    meta:\n        author = \"NCSC\"\n\n description = \"This rule detects strings present in the AUTHENTIC ANTICS .NET\nstealer payload.\"\n\n    strings:\n        $ = \"http://www.gstatic.com/generate_204\" wide\n        $ = \"d3590ed6-52b3-4102-aeff-aad2292ab01c\" wide\n        $ = \"https://login.microsoftonline.com/common/oauth2/token\" wide\n        $ = \"https://outlook.office365.com/api/v2.0/me/sendMail\" wide\n        $ =\n\"https://login.microsoftonline.com/common/oauth2/authorize?response_type=code+id_token&c\nlient_id=d3590ed6-52b3-4102-aeff-aad2292ab01c&redirect_uri=ms-appx-\nweb%3a%2f%2fMicrosoft.AAD.BrokerPlugin%2fd3590ed6-52b3-4102-aeff-\naad2292ab01c&instance_aware=true&nonce=\" wide\n\nweb://Microsoft.AAD.BrokerPlugin/d3590ed6-52b3-4102-aeff-aad2292ab01c\" wide\n\n$ = \"<form method=\\\"POST\\\" name=\\\"hiddenform\\\" action=\\\"ms-appx-\n\n    condition:\n        (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550 and all of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-11-10T11:30:37.29258Z"
        },
        {
