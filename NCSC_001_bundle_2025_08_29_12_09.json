        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--38550d30-64d8-4e69-9b10-792ca5780f89",
            "created_by_ref": "identity--e20ae9ce-8a39-4091-b8a3-1bb0d324d9fe",
            "created": "2025-08-29T12:09:58.420164Z",
            "modified": "2025-08-29T12:09:58.420164Z",
            "name": "umbrella stand",
            "description": "UMBRELLA STAND is a medium sophistication malware targeting Fortinet devices, specifically FortiGate 100D series firewalls. It is composed of multiple interconnected binaries and components, deployed to facilitate long-term access into target networks. The malware beacons to its C2 server using fake TLS on port 443, supports remote shell execution, configurable beacon frequency and C2 IP address, and uses AES-encrypted C2 communications. It hooks the reboot functionality for persistence, uses hidden directories and generic filenames for evasion, and employs AES-encrypted stack strings to hinder detection and analysis. Associated tooling includes process injection, file encryption/decryption, and monitoring components.",
            "malware_types": [
                "remote-access-trojan",
                "backdoor"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
