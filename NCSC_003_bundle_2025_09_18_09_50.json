        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--36782fd2-306a-4b38-9e1f-79a064169fcb",
            "created_by_ref": "identity--6fbaae66-2d81-4acb-9e59-41aa47bf4ff3",
            "created": "2025-09-18T09:50:32.646587Z",
            "modified": "2025-09-18T09:50:32.646587Z",
            "name": "damascened peacock",
            "description": "DAMASCENED PEACOCK is a lightweight, staged downloader targeting Windows, delivered via spear-phishing. It is implemented as a 32-bit (x86) DLL with a single export named DllExport containing the malicious functionality. It is responsible for downloading and executing the final stage via a COM Hijack, and implements defence evasion techniques such as XOR-based string obfuscation and dynamic resolution of Win32 APIs. It is a variant of RustyClaw and SnipBot, and part of the wider RomCom malware family. DAMASCENED PEACOCK is the second of three stages in the infection chain.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
        {
