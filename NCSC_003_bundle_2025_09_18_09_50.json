        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--7d5fc186-676f-4011-b2e9-9fdb6311bed4",
            "created_by_ref": "identity--6fbaae66-2d81-4acb-9e59-41aa47bf4ff3",
            "created": "2025-09-18T09:50:32.649601Z",
            "modified": "2025-09-18T09:50:32.649601Z",
            "name": "DAMASCENED_PEACOCK_resolve_apis",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule DAMASCENED_PEACOCK_resolve_apis {\n    meta:\n        author = \"NCSC\"\n        description = \"Targets a code pattern which is used to resolve\nAPIs in the first stage which loads DAMASCENED PEACOCK.\"\n        hash1 = \"1bd5b9fec7ccc29144989203c3b15a10c9c22a76\"\n\n    strings:\n\n    $ = {56 31 C0 85 D2 74 1D 0F B6 31 41 4A 69 F6 [4] 0F AF F6 C1\n\nC6 10 31 C6 0F AF F6 C1 C6 10 89 F0 EB DF 5E C3}\n    condition:\n\n    (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550\n\nand all of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-09-18T09:50:32.649585Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--18156d59-3d54-4047-950e-83d4bf303f3d",
            "created_by_ref": "identity--6fbaae66-2d81-4acb-9e59-41aa47bf4ff3",
            "created": "2025-09-18T09:50:32.649947Z",
            "modified": "2025-09-18T09:50:32.649947Z",
            "name": "DAMASCENED_PEACOCK_winhttpconnect",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule DAMASCENED_PEACOCK_winhttpconnect {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule targets a code pattern in DAMASCENED\nPEACOCK which sees the resolved pointer for WinHttpConnect retrieved from\na struct, arguments pushed e.g. 8080 as the port, and then a call.\"\n        hash1 = \"8c25b982e5e4b3418b779c85b8ac8a6acf393063\"\n\n    strings:\n        $ = {8B 85 90 00 00 00 6A 00 68 90 1F 00 00 FF 76 34 FF 33 FF D0}\n    condition:\n        (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550 and\nall of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-09-18T09:50:32.649936Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--e50c5164-53ef-4130-8513-99a0d1af9c88",
            "created_by_ref": "identity--6fbaae66-2d81-4acb-9e59-41aa47bf4ff3",
            "created": "2025-09-18T09:50:32.650298Z",
            "modified": "2025-09-18T09:50:32.650298Z",
            "name": "DAMASCENED_PEACOCK_API_hashes",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule DAMASCENED_PEACOCK_API_hashes {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule targets code patterns for pushing the\nhash onto the stack and then moving the returned pointer into a structure\nin DAMASCENED PEACOCK.\"\n        hash1 = \"8c25b982e5e4b3418b779c85b8ac8a6acf393063\"\n\n    strings:\n        $ = {68 1A 81 A8 01} // push hash\n        $ = {83 C4 04 89 46 04} // move result into struct\n\n        $ = {68 7D 53 AE 52}\n        $ = {83 C4 04 89 46 08}\n\n        $ = {68 59 6F 3C 00}\n        $ = {83 C4 04 89 46 0C}\n\n        $ = {68 6B 9A C6 E1}\n        $ = {83 C4 04 89 46 10}\n\n        $ = {68 32 A9 7C 08}\n        $ = {83 C4 04 89 46 14}\n\n        $ = {68 76 EE 30 8A}\n        $ = {83 C4 04 89 46 18}\n\n    condition:\n        (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550 and\nall of them\n\n}",
            "pattern_type": "yara",
            "valid_from": "2025-09-18T09:50:32.650289Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--4464d627-60bc-4b23-a5fd-921e197b8749",
            "created_by_ref": "identity--6fbaae66-2d81-4acb-9e59-41aa47bf4ff3",
            "created": "2025-09-18T09:50:32.650638Z",
            "modified": "2025-09-18T09:50:32.650638Z",
            "name": "DAMASCENED_PEACOCK_API_hashing_algorithm",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule DAMASCENED_PEACOCK_API_hashing_algorithm {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule targets the code in DAMASCENED PEACOCK\nused for generating an API hash. The rule is agnostic of the constants\nused to generate the hash.\"\n        hash1 = \"8c25b982e5e4b3418b779c85b8ac8a6acf393063\"\n\n    strings:\n        $ =\n{8D0C0669C9[4]8D5201C1C9??81C1[4]03C1C1C8??0FAFC60FBE3203C08BC8C1C8??C1C9\n??85F6}\n    condition:\n        any of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-09-18T09:50:32.650629Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--94c9e547-4b85-4584-9727-fb7a8330cc34",
            "created_by_ref": "identity--6fbaae66-2d81-4acb-9e59-41aa47bf4ff3",
            "created": "2025-09-18T09:50:32.650935Z",
            "modified": "2025-09-18T09:50:32.650935Z",
            "name": "DAMASCENED_PEACOCK_strings",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule DAMASCENED_PEACOCK_strings {\n    meta:\n        author = \"NCSC\"\n        description = \"This rule targets the original name and export\nname in DAMASCENED PEACOCK.\"\n        hash1 = \"8c25b982e5e4b3418b779c85b8ac8a6acf393063\"\n\n    strings:\n        $ = \"XcsExplorerDll_x86.dll\"\n        $ = \"DllExport\"\n    condition:\n        (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550 and\nall of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-09-18T09:50:32.650927Z"
        },
