        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--c36be43a-6ae7-40cd-b089-a43911a9ed29",
            "created_by_ref": "identity--ab17afe5-a3a3-42b3-be7c-3579615fcaf0",
            "created": "2025-11-08T09:35:34.415559Z",
            "modified": "2025-11-08T09:35:34.415559Z",
            "name": "small sieve",
            "description": "Small Sieve is a Python 3.9 backdoor distributed via a large NSIS installer (gram_app.exe) that installs and persists a PyInstaller-bundled backdoor binary (index.exe). It leverages the Telegram Bot API for beaconing and tasking over HTTPS, with custom hex byte-shuffling and obfuscated Base64 routines to evade detection. The malware supports limited functionality focused on maintaining access: downloading files via urllib, executing arbitrary commands through cmd.exe, changing Telegram API tokens, and disconnecting/reconnecting. Persistence is achieved via a Registry Run key that launches the payload with a required guardrail argument ('Platypus'). It also employs masquerading with filenames mimicking Microsoft/Outlook names, and stores obfuscated strings and updated Telegram tokens locally.",
            "malware_types": [
                "backdoor",
                "downloader"
            ],
            "is_family": true
        },
