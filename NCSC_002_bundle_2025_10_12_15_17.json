        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--3d7d5a54-f28f-414b-9f4c-acff83c99817",
            "created_by_ref": "identity--c9633a51-4c4f-45c0-b475-00603358072d",
            "created": "2025-10-12T15:17:51.324957Z",
            "modified": "2025-10-12T15:17:51.324957Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware that runs within the Outlook process on Windows. It displays malicious login prompts to steal credentials and OAuth 2.0 tokens, which are then used to access victim email accounts. The malware employs extensive defense evasion techniques, including environmental keying, string obfuscation, and unhooking of ntdll.dll functions. It masquerades as the Microsoft Authentication Library (MSAL) for .NET, includes a dropper and stealer payload, and exfiltrates data by sending emails from the victim\u2019s account to an actor-controlled address. Persistence is achieved via COM hijacking, and the malware communicates exclusively with legitimate services.",
            "malware_types": [
                "spyware",
                "backdoor"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--b96140ad-4cff-415d-8562-15e4e8673870",
            "created_by_ref": "identity--c9633a51-4c4f-45c0-b475-00603358072d",
            "created": "2025-10-12T15:17:51.325345Z",
            "modified": "2025-10-12T15:17:51.325345Z",
            "name": "microsoft.identity64.dll",
            "description": "A dropper component of AUTHENTIC ANTICS responsible for implementing defense evasion techniques, decrypting, and loading the credential-stealing payload (Microsoft.Identity.Client.dll). It is a 64-bit DLL built as a mixed-mode assembly and is executed and persisted via COM hijacking.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--f161271c-09ce-4891-8bcf-e81ade52650f",
            "created_by_ref": "identity--c9633a51-4c4f-45c0-b475-00603358072d",
            "created": "2025-10-12T15:17:51.325644Z",
            "modified": "2025-10-12T15:17:51.325644Z",
            "name": "microsoft.identity.client.dll",
            "description": "A credential and OAuth 2.0 token stealer payload loaded by Microsoft.Identity64.dll as part of AUTHENTIC ANTICS. It generates login prompts to intercept credentials and tokens, which are then exfiltrated via the victim\u2019s Outlook account.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
