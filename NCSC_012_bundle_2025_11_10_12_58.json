        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--8aed2fb2-56eb-4b88-a30b-5c3cb7d91add",
            "created_by_ref": "identity--5c41224f-9203-44b9-8aaa-254181b208e2",
            "created": "2025-11-10T12:58:05.797838Z",
            "modified": "2025-11-10T12:58:05.797838Z",
            "name": "small sieve",
            "description": "Small Sieve is a simple Python backdoor distributed using an NSIS installer that performs persistence. It provides basic functionality to maintain and expand a foothold in victim infrastructure using custom string and network traffic obfuscation schemes, together with the Telegram Bot API for command and control. The malware is limited to file download and command line execution and is designed to evade detection by network defenders. It beacons using the Telegram Bot API and supports commands like deleting the backdoor, downloading files, changing Telegram tokens, disconnecting, and executing arbitrary shell commands. Internal strings and Telegram tokens are obfuscated using a custom alphabet and encoded with Base64. Persistence is handled via a registry run key, and session information is stored in an obfuscated file.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
