        {
            "type": "attack-pattern",
            "spec_version": "2.1",
            "id": "attack-pattern--8af11bec-c88c-4f35-9afe-cd06cadc7759",
            "created_by_ref": "identity--06bb4ab7-186b-4e6a-aee7-af2fef954b80",
            "created": "2025-10-12T21:50:17.367571Z",
            "modified": "2025-10-12T21:50:17.367571Z",
            "name": "Forced Authentication",
            "description": "AUTHENTIC ANTICS creates a pop-up browser window from within Outlook and directs it to Microsoft\u2019s OAuth 2.0 /authorize endpoint, prompting the victim to enter credentials (mirroring legitimate software behavior) to capture credentials and authorization codes.",
            "kill_chain_phases": [
