        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--ccabf646-1c7c-417f-8d97-ff9e6aae4c0c",
            "created_by_ref": "identity--3e84e203-2030-49a9-9b86-35c77d23da8f",
            "created": "2025-11-07T12:16:48.596487Z",
            "modified": "2025-11-07T12:16:48.596487Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a multi-stage Windows malware that runs inside the Outlook process to steal Microsoft Office credentials and OAuth 2.0 tokens. It displays Outlook-like login prompts, intercepts authorization codes via a controlled browser window (Navigating event), and redeems them for access and refresh tokens. It persists via COM hijacking, employs environmental keying (deriving an AES-256 decryption key from MachineGuid and volume serial), unhooks registry-related APIs in ntdll.dll, and hooks IEConfiguration_GetBool in iertutil.dll. The stealer component masquerades as Microsoft\u2019s MSAL for .NET (codebase included but unused) and exfiltrates collected data by sending emails from the victim\u2019s account through the Outlook web API without leaving sent items. Network traffic is limited to legitimate services (Microsoft OAuth endpoints and Outlook API).",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
