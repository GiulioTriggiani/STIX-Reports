        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--6c63f299-cad6-4c6f-90ea-80dcd87d6f3f",
            "created_by_ref": "identity--5b23f5fb-3d29-41da-a1e2-eda557c931de",
            "created": "2025-11-11T16:08:10.163688Z",
            "modified": "2025-11-11T16:08:10.163688Z",
            "name": "rhythmic parry",
            "description": "Rhythmic Parry is a 64-bit downloader targeting the Windows operating system. It is configured by a previous stage to be run as a scheduled task via rundll32.exe at each logon as SYSTEM. It contains succinct functionality to download, decrypt and execute a payload in memory.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--f244b835-6801-4ff0-bd5b-65449cbce281",
            "created_by_ref": "identity--5b23f5fb-3d29-41da-a1e2-eda557c931de",
            "created": "2025-11-11T16:08:10.164399Z",
            "modified": "2025-11-11T16:08:10.164399Z",
            "name": "rp_config_strings",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule rp_config_strings { meta: author = \"NCSC\" description = \"Base64 and XOR encoded strings which in plaintext appear in both samples of Rhythmic Parry.\" date = \"2022-01-10\" hash1 = \"628cf9ace06af1d0985fce3c5c39ab0773135c00\" hash2 = \"af895002b93854de0eb6f17dc4823a50c7bd08f5\" strings: $LD_UA = \"...\" wide $DV_UA = \"...\" wide $LD_EXP = \"...\" wide $DV_EXP = \"...\" wide condition: uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and (2 of ($LD_*) or 2 of ($DV_*)) }",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T16:08:10.164388Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--34621fc4-efc7-407b-88f5-7c944597fbc9",
            "created_by_ref": "identity--5b23f5fb-3d29-41da-a1e2-eda557c931de",
            "created": "2025-11-11T16:08:10.164726Z",
            "modified": "2025-11-11T16:08:10.164726Z",
            "name": "rp_junk_code",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule rp_junk_code { meta: author = \"NCSC\" description = \"Highlights common junk code sections identified in both Rhythmic Parry samples.\" date = \"2022-01-10\" hash1 = \"628cf9ace06af1d0985fce3c5c39ab0773135c00\" hash2 = \"af895002b93854de0eb6f17dc4823a50c7bd08f5\" strings: $post_writefile = {...} $temp_path_a = {...} condition: uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and filesize > 500KB and (#temp_path_a > 30) and (#post_writefile > 12) }",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T16:08:10.164719Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--70714efb-6bdf-42c9-81e8-453d6d324160",
            "created_by_ref": "identity--5b23f5fb-3d29-41da-a1e2-eda557c931de",
            "created": "2025-11-11T16:08:10.165016Z",
            "modified": "2025-11-11T16:08:10.165016Z",
            "name": "rp_import_obfuscation",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule rp_import_obfuscation { meta: author = \"NCSC\" description = \"Import obfuscation is achieved by pushing immediate character values onto the stack while applying single byte global XOR to each, the global XOR is reinitiated each time it is applied. This rule highlights the four-instruction sequence to achieve this.\" date = \"2022-01-10\" hash1 = \"628cf9ace06af1d0985fce3c5c39ab0773135c00\" hash2 = \"af895002b93854de0eb6f17dc4823a50c7bd08f5\" strings: $char_sequence_eax = {...} condition: uint16(0) == 0x5A4D and uint32(uint32(0x3C)) == 0x00004550 and #char_sequence_eax > 20 }",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T16:08:10.165008Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--ed993acd-0bd2-4554-8246-e7db991dda22",
            "created_by_ref": "identity--5b23f5fb-3d29-41da-a1e2-eda557c931de",
            "created": "2025-11-11T16:08:10.165312Z",
            "modified": "2025-11-11T16:08:10.165312Z",
            "name": "rp_hash_payload",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule rp_hash_payload { meta: author = \"NCSC\" description = \"Hash comparison routine used to check downloaded payload in Rhythmic Parry samples.\" date = \"2022-01-10\" hash1 = \"628cf9ace06af1d0985fce3c5c39ab0773135c00\" hash2 = \"af895002b93854de0eb6f17dc4823a50c7bd08f5\" strings: $hash_1 = {...} $hash_2 = {...} $hash_3 = {...} condition: (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550 and all of them }",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T16:08:10.165288Z"
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--fd6ce3e6-4b32-4363-b1bb-e051c74096b6",
            "created_by_ref": "identity--5b23f5fb-3d29-41da-a1e2-eda557c931de",
            "created": "2025-11-11T16:08:10.16562Z",
            "modified": "2025-11-11T16:08:10.16562Z",
            "name": "rp_xor_routine",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule rp_xor_routine { meta: author = \"NCSC\" description = \"Targets the XOR routine used to decode configuration strings in both observed Rhythmic Parry samples.\" date = \"2022-01-10\" hash1 = \"628cf9ace06af1d0985fce3c5c39ab0773135c00\" hash2 = \"af895002b93854de0eb6f17dc4823a50c7bd08f5\" strings: $xor_1 = {...} $xor_2 = {...} $xor_3 = {...} condition: (uint16(0) == 0x5A4D) and uint32(uint32(0x3C)) == 0x00004550 and 2 of them }",
            "pattern_type": "yara",
            "valid_from": "2025-11-11T16:08:10.165614Z"
        },
