        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--7a5ce6c6-73b5-4190-9bf1-9aa58546c400",
            "created_by_ref": "identity--bf6a6be5-3389-463e-93bc-e0a0797cd213",
            "created": "2025-09-05T09:55:33.801447Z",
            "modified": "2025-09-05T09:55:33.801447Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware that targets the Windows Operating System, specifically running within the Outlook process. It displays malicious login prompts to steal credentials and OAuth 2.0 tokens, which are then used to access victim email accounts. The malware employs extensive defense evasion techniques, including environmental keying, string obfuscation, and unhooking suspected hooks from within ntdll.dll. It masquerades as the Microsoft Authentication Library (MSAL) for .NET, embedding the codebase but only using it for legitimacy. Exfiltration is performed by sending emails from the victim\u2019s account to an actor-controlled email address, with emails not appearing in the sent folder. The malware does not communicate with a command-and-control server and is persisted via COM hijacking.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
            "id": "identity--fb61b055-df03-4ee8-b98a-9fe0781413d1",
