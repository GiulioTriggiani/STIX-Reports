        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--96f7eed3-0d4a-4610-b154-0f6670830089",
            "created_by_ref": "identity--6dbc388a-214b-4de1-94f8-82ccc352697a",
            "created": "2025-11-11T20:09:41.509153Z",
            "modified": "2025-11-11T20:09:41.509153Z",
            "name": "authentic antics",
            "description": "A highly targeted credential and OAuth 2.0 token-stealing malware targeting the Windows Operating System and Outlook. It runs within the Outlook process, displaying malicious login prompts to steal credentials and tokens. It employs extensive defense evasion techniques such as environmental keying and ntdll.dll unhooking. It exfiltrates data by sending emails from the victim's account.",
            "malware_types": [
                "spyware",
                "remote-access-trojan"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--847cc365-e873-40de-a9ff-49245415154f",
            "created_by_ref": "identity--6dbc388a-214b-4de1-94f8-82ccc352697a",
            "created": "2025-11-11T20:09:41.509505Z",
            "modified": "2025-11-11T20:09:41.509505Z",
            "name": "microsoft.identity64.dll",
            "description": "A dropper component of AUTHENTIC ANTICS responsible for implementing defence evasion techniques, decrypting and loading the credential stealing payload, Microsoft.Identity.Client.dll. It is a 64-bit DLL built as a mixed-mode assembly.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--9191aaa9-2f36-459e-8051-0a4740dacaa9",
            "created_by_ref": "identity--6dbc388a-214b-4de1-94f8-82ccc352697a",
            "created": "2025-11-11T20:09:41.509767Z",
            "modified": "2025-11-11T20:09:41.509767Z",
            "name": "microsoft.identity.client.dll",
            "description": "The credential and OAuth 2.0 token stealer payload of AUTHENTIC ANTICS. It is a 64-bit .NET DLL loaded by Microsoft.Identity64.dll and is not present on disk. It generates login prompts to intercept and exfiltrate credentials and tokens.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
