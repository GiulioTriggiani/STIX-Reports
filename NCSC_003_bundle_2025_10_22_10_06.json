        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--d867ed05-ef9a-4617-a465-be225d42ff2f",
            "created_by_ref": "identity--0b96e299-8dce-4374-8f9e-8ddf206fc18c",
            "created": "2025-10-22T10:06:44.776072Z",
            "modified": "2025-10-22T10:06:44.776072Z",
            "name": "damascened peacock",
            "description": "DAMASCENED PEACOCK is a lightweight, staged downloader targeting Windows, observed in late 2024 spear-phishing campaigns against the UK Ministry of Defence. It is implemented as a 32-bit DLL with a single export (DllExport) and serves as the second of three stages. The first stage (code-signed, masquerading as a PDF) downloads and loads DAMASCENED PEACOCK, which then downloads the final stage from its C2 and achieves its execution via a COM hijack into explorer.exe. The malware employs defense evasion techniques including XOR-based string obfuscation and dynamic resolution of Win32 APIs using an API hashing algorithm. It communicates over HTTP (port 8080) to apimonger[.]com, selecting the appropriate 32-bit or 64-bit final-stage payload (get_update_manager32/64) after checking host architecture.",
            "malware_types": [
                "downloader"
            ],
            "is_family": true
        },
