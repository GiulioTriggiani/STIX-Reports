        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--2a553053-01e6-4b0c-bca3-ae39985a9beb",
            "created_by_ref": "identity--3d11e7ff-c861-4414-9acf-fdd3b1ea89cc",
            "created": "2025-09-03T16:32:56.609411Z",
            "modified": "2025-09-03T16:32:56.609411Z",
            "name": "jaguar tooth",
            "description": "Jaguar Tooth is non-persistent malware that targets Cisco IOS routers. It collects device information and exfiltrates it over Trivial File Transfer Protocol (TFTP), and enables unauthenticated backdoor access by patching authentication routines. It is deployed and executed via exploitation of the patched SNMP vulnerability CVE-2017-6742. Jaguar Tooth is composed of payloads and patches, including a process called Service Policy Lock that automatically collects and exfiltrates device information such as running configuration, firmware version, directory listing of flash memory, and network information. The malware modifies the system\u2019s authentication process, allowing unauthenticated access to any local account for any provided password via Telnet and physical sessions.",
            "malware_types": [
                "backdoor"
            ],
            "is_family": true
        },
        {
            "type": "indicator",
            "spec_version": "2.1",
            "id": "indicator--b841b502-64ea-4f08-bd52-b1113452ff5a",
            "created_by_ref": "identity--3d11e7ff-c861-4414-9acf-fdd3b1ea89cc",
            "created": "2025-09-03T16:32:56.609899Z",
            "modified": "2025-09-03T16:32:56.609899Z",
            "name": "JaguarTooth_Cisco_IOS_payload",
            "description": "YARA rule to detect related activity.",
            "pattern": "rule JaguarTooth_Cisco_IOS_payload {\n    meta:\n        author = \"NCSC\"\n        description = \"This signature detects the Jaguar Tooth Cisco IOS\nmalware. It looks for the process name, two of the hard-coded commands,\nand code calling two Cisco IOS functions.\"\n    strings:\n        $ = \"Service Policy Lock\"\n        $ = \"sleep 5000\"\n        $ = \"tclquit\"\n        $ = {0C ?? ?? ?? 00 00 30 25 0C ?? ?? ?? 24 04 FF FF 8F BF 00 34}\n    condition:\n        3 of them\n}",
            "pattern_type": "yara",
            "valid_from": "2025-09-03T16:32:56.609886Z"
        },
