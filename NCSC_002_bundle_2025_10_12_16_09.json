        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--df490c96-d8c7-4f77-aa7c-73505d9bf4f9",
            "created_by_ref": "identity--2d70991e-ac9e-4b83-9356-726443acefe9",
            "created": "2025-10-12T16:09:44.251183Z",
            "modified": "2025-10-12T16:09:44.251183Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware that runs within the Outlook process on Windows. It displays malicious login prompts to steal credentials and OAuth 2.0 tokens, which are then used to access victim email accounts. The malware employs extensive defense evasion techniques, including environmental keying, string obfuscation, and unhooking of ntdll.dll functions. It masquerades as the Microsoft Authentication Library (MSAL) for .NET, exfiltrates data via emails sent from the victim\u2019s account to an actor-controlled address, and persists via COM hijacking. Network communications are exclusively with legitimate services, and the malware does not implement traditional command and control.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
        {
            "type": "identity",
            "spec_version": "2.1",
