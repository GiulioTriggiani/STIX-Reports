        {
            "type": "malware",
            "spec_version": "2.1",
            "id": "malware--cabfad82-134d-4201-83d3-987fc091e01e",
            "created_by_ref": "identity--34e22713-e742-4328-8665-48f013290b0f",
            "created": "2025-10-22T10:01:03.577153Z",
            "modified": "2025-10-22T10:01:03.577153Z",
            "name": "authentic antics",
            "description": "AUTHENTIC ANTICS is a highly targeted credential and OAuth 2.0 token-stealing malware that runs within the Microsoft Outlook process on Windows. It presents malicious login prompts to intercept Microsoft Office credentials and authorization codes, which it redeems for access and refresh tokens to maintain access to victims\u2019 email accounts. It comprises a mixed-mode dropper (Microsoft.Identity64.dll) that performs extensive defense evasion (including string obfuscation, environmental keying, ntdll.dll unhooking, and a hook in iertutil.dll) and a .NET stealer payload (Microsoft.Identity.Client.dll) that abuses OAuth flows. The malware communicates exclusively with legitimate services (Microsoft OAuth endpoints and Outlook web API), persists via COM hijacking, stores a stolen refresh token in an Outlook logging registry key, and exfiltrates gzip-compressed and RSA-encrypted data by sending email through the victim\u2019s Outlook account with SaveToSentItems=false so it does not appear in Sent Items. It employs execution throttling (every six days) and only targets ADAL-authenticated accounts.",
            "malware_types": [
                "spyware"
            ],
            "is_family": true
        },
